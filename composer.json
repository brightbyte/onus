{
  "name": "wikimedia/onus",
  "description": "Identify code that was frequently changed to fix production errors",
  "license": "GPL-2.0-or-later",
  "prefer-stable": true,
  "require": {
    "ext-json": "*",
    "gitonomy/gitlib": "^1.2",
    "symfony/console": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Wikimedia\\Onus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wikimedia\\Onus\\Test\\": "tests/"
    }
  },
  "scripts": {
    "onus": "bin/onus"
  },
  "config": {
    "optimize-autoloader": true,
    "prepend-autoloader": false
  }
}
